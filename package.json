{
  "name": "eBiblijaApp",
  "version": "1.0.0",
  "description": "Electron wrapper for a html Macedonian bible",
  "main": "main.js",
  "homepage": "./",
  "license": "free",
  "author": "Nikolche Kolevski",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolchek14/eBiblijaApp.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/nikolchek14/eBiblijaApp/issues"
  },
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "build": {
    "appId": "com.electron.eBiblijaApp",
    "compression": "normal",
    "asar": true,
    "extends": null,
    "files": [
      "main.js",
      "index.html",
      "build/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": "portable",
      "icon": "bbl.png"
    }
  },
  "config": {
  }
}
